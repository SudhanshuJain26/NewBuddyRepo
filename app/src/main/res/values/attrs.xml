<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GIFView">
        <attr name="src" format="reference" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="bar_length" format="dimension" />
        <attr name="delayMillis" format="dimension"/>
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="float"/>
    </declare-styleable>
    <declare-styleable name="InkView">
        <!-- Feature flags for InkView. Defaults to "interpolation|responsiveWeight" -->
        <attr name="inkFlags">
            <!-- No features turned on -->
            <flag name="none" value="0"/>
            <!-- Interpolate paths as cubic-bezier curves -->
            <flag name="interpolation" value="1"/>
            <!-- Stroke width changes in response to velocity -->
            <flag name="responsiveWeight" value="2"/>
        </attr>
    </declare-styleable>
    

</resources>


